cat > /boot/1.YAML << 'EOF'
services:
  intellissh:
    image: clusterzx/intellissh:latest
    container_name: intellissh
    ports:
      - "8080:3000"
    volumes:
      - ./intellissh-data:/app/server/data
    restart: always
EOF

mkdir -p ./intellissh-data && docker compose -f /boot/1.YAML up -d
